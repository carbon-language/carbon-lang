define void @unreachable_block() {
; CHECK-LABEL: @unreachable_block(
; CHECK-NEXT:  bb.0:
; CHECK-NEXT:    br label [[BB_1:%.*]]
; CHECK:       dead:
; CHECK-NEXT:    [[T0:%.*]] = add i16 [[T0]], undef
; CHECK-NEXT:    br label [[BB_1]]
; CHECK:       bb.1:
; CHECK-NEXT:    [[T1:%.*]] = phi i16 [ undef, [[BB_0:%.*]] ], [ [[T0]], [[DEAD:%.*]] ]
; CHECK-NEXT:    ret void
;
