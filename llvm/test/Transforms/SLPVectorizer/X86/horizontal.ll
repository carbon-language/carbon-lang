; STORE-NEXT:  bb.0:
; STORE-NEXT:    br label [[BB_1:%.*]]
; STORE:       dead:
; STORE-NEXT:    [[T0:%.*]] = add i16 [[T0]], undef
; STORE-NEXT:    br label [[BB_1]]
; STORE:       bb.1:
; STORE-NEXT:    [[T1:%.*]] = phi i16 [ undef, [[BB_0:%.*]] ], [ [[T0]], [[DEAD:%.*]] ]
; STORE-NEXT:    ret void
;
