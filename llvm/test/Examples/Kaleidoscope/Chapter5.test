# RUN: Kaleidoscope-Ch5 < %s 2>&1 | FileCheck %s
# REQUIRES: default_triple

# Test 'if' expression.
def foo(x) if x < 10 then 0 else 1;
foo(9);
foo(11);
# CHECK: Evaluated to 0.000000
# CHECK: Evaluated to 1.000000

# Test 'for' expression.
extern printd(x);
for i = 1, i < 5, 1.0 in
  printd(i);
# CHECK: 1.0
# CHECK: 2.0
# CHECK: 3.0
# CHECK: 4.0
# CHECK: 5.0
# CHECK: Evaluated to 0.000000
