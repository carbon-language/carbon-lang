# RUN: yaml2obj %s > %t
# RUN: not llvm-elfabi --elf %t --emit-tbe=%t.tbe 2>&1 | FileCheck %s

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_DYN
  Machine:         EM_X86_64
Sections:
  - Name:            .dynstr
    Type:            SHT_STRTAB
    Flags:           [ SHF_ALLOC ]
    Address:         0x1000
    Content:         "00"
  - Name:            .dynamic
    Type:            SHT_DYNAMIC
    Flags:           [ SHF_ALLOC ]
    Address:         0x0000000000001008
    Link:            .dynstr
    AddressAlign:    0x0000000000000008
    EntSize:         0x0000000000000010
    Entries:
      - Tag:             DT_SONAME
        Value:           0x0000000000000000
      - Tag:             DT_STRSZ
        Value:           0x0000000000000001
      - Tag:             DT_STRTAB
        Value:           0x0000000000000260 # Bad vaddr (no PT_LOAD for 0x0000 to 0x0FFF)
      - Tag:             DT_SYMTAB
        Value:           0x0000000000001000
      - Tag:             DT_NULL
        Value:           0x0000000000000000
ProgramHeaders:
  - Type: PT_LOAD
    Flags: [ PF_R ]
    VAddr: 0x1000
    PAddr: 0x1000
    Align: 8
    Sections:
      - Section: .dynstr
      - Section: .dynamic
  - Type: PT_DYNAMIC
    Flags: [ PF_X, PF_R ]
    VAddr: 0x1008
    PAddr: 0x1008
    Sections:
      - Section: .dynamic

# CHECK: Virtual address is not in any segment when locating .dynstr section contents
