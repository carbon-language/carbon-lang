        Value: 0x10
      - Tag:   DT_HEXAGON_VER
        Value: 0x1000
      - Tag:   DT_HEXAGON_PLT
        Value: 0x1000
      - Tag:   DT_NULL
        Value: 0
ProgramHeaders:
  - Type: PT_LOAD
    VAddr: 0x1000
    Sections:
      - Section: .dynstr
      - Section: .dynamic
  - Type: PT_DYNAMIC
    VAddr: 0x1010
