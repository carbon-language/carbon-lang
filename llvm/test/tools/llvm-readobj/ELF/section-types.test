    Type: SHT_LLVM_DEPENDENT_LIBRARIES
  - Name: .llvm_sympart.f
    Type: SHT_LLVM_SYMPART
  - Name: part1
    Type: SHT_LLVM_PART_EHDR
  - Name: .phdrs
    Type: SHT_LLVM_PART_PHDR
