CHECK: [[AOFF:0x[0-9a-f]*]]:

CHECK-LABEL: Compile Unit: length = {{.*}} version = 0x0004
CHECK: DW_TAG_compile_unit
CHECK:   DW_AT_name {{.*}} "c.cpp"
CHECK:   DW_TAG_subprogram
CHECK:     DW_AT_name {{.*}} "c"

CHECK-LABEL: Compile Unit: length = {{.*}} version = 0x0004
CHECK: DW_TAG_compile_unit
CHECK:   DW_AT_name {{.*}} "e.cpp"
