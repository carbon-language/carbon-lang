# RUN: llvm-objcopy --rename-section=.foo=.bar,debug  \
# RUN:   --rename-section=.baz=.blah,debug %t %t.debug
# RUN: llvm-readobj --sections %t.debug | FileCheck %s --check-prefixes=CHECK,PROGBITS,WRITE
