# RUN: yaml2obj %s > %t
# RUN: llvm-objcopy -only-section=.test %t %t2
# RUN: llvm-objcopy -j .test %t %t3
# RUN: llvm-readobj -file-headers -sections %t2 | FileCheck %s
# RUN: diff %t2 %t3

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .test
    Type:            SHT_PROGBITS
    Flags:           [ ]

# CHECK: SectionHeaderCount: 5

# CHECK:     Name: .test
# CHECK:     Name: .symtab
# CHECK:     Name: .strtab
# CHECK:     Name: .shstrtab
