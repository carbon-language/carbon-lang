# RUN: yaml2obj %s > %t
# RUN: not llvm-objcopy --discard-locals %t %t2 2>&1 | FileCheck %s

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .text
    Type:            SHT_PROGBITS
  - Name:            .rel.text
    Type:            SHT_REL
    Link:            .symtab
    Info:            .text
    Relocations:
      - Offset: 0x1000
        Symbol: .L.rel
        Type:   R_X86_64_PC32
Symbols:
  Local:
    - Name:     .L.rel
      Type:     STT_FUNC
      Section:  .text

# CHECK: not stripping symbol '.L.rel' because it is named in a relocation.
