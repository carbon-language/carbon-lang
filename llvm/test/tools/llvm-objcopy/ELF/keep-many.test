# RUN: yaml2obj %s > %t
# RUN: llvm-objcopy --strip-non-alloc --keep-section=.test --keep-section=.test3 %t %t2
# RUN: llvm-objcopy --strip-non-alloc --regex --keep-section='^.test[0-9]+$' %t %t3
# RUN: llvm-readobj --file-headers --sections %t2 | FileCheck %s
# RUN: llvm-readobj --file-headers --sections %t3 | FileCheck %s --check-prefix=REGEX

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .test
    Type:            SHT_PROGBITS
    Flags:           [ ]
  - Name:            .test2
    Type:            SHT_PROGBITS
    Flags:           [ ]
  - Name:            .test3
    Type:            SHT_PROGBITS
    Flags:           [ ]


# CHECK: SectionHeaderCount: 4

# CHECK:     Name: .test
# CHECK:     Name: .test3
# CHECK:     Name: .shstrtab

# REGEX: SectionHeaderCount: 4
# REGEX:     Name: .test2
# REGEX:     Name: .test3
# REGEX:     Name: .shstrtab
