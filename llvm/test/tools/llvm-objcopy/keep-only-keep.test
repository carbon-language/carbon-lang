# RUN: yaml2obj %s > %t
# RUN: llvm-objcopy -keep=.test2 -only-keep=.test %t %t2
# RUN: llvm-objcopy -j=.test -keep=.test2 %t %t3
# RUN: llvm-readobj -file-headers -sections %t2 | FileCheck %s
# RUN: diff %t2 %t3

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .test
    Type:            SHT_PROGBITS
  - Name:            .test2
    Type:            SHT_PROGBITS
  - Name:            .test3
    Type:            SHT_PROGBITS

# CHECK: SectionHeaderCount: 6

# CHECK:     Name: .test
# CHECK:     Name: .test2
# CHECK:     Name: .symtab
# CHECK:     Name: .strtab
# CHECK:     Name: .shstrtab
