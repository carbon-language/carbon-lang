#include <stdio.h>

int bar(int x, int y) {
  if (x % 2) {
    return x - y;
  }
  return x + y;
}

void foo() {
  int s, i = 0;
  while (i++ < 4000 * 4000)
    if (i % 3) s = bar(i, s); else s += bar(s, i);
  printf("sum is %d\n", s);
}

int main() {
  foo();
  return 0;
}
