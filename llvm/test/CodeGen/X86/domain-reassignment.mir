    %8 = IMPLICIT_DEF
    %9 = INSERT_SUBREG %8, %18, 1
    %10 = COPY %9
    %11 = VMOVAPDZrrk %2, killed %10, %1
    VMOVAPDZmr %0, 1, _, 0, _, killed %11 

    ; CHECK: KTESTBrr %18, %18, implicit-def %eflags
