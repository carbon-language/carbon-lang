#   char *p3 = (char *)p1;
#   *p3 = 0;
#   alpha t = *p1;
#   *p2 = t;
# }
#
# Using the command line:
# clang -g -c 1.cpp -O2 -S -emit-llvm -fno-strict-aliasing --target=x86_64-unknown-unknown -o test.ll
# llc -stop-before=x86-avoid-SFB test.ll -o before.mir

