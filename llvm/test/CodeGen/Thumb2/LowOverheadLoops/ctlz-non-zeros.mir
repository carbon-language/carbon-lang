    %bitcast = bitcast <16 x i8> %shrn to <8 x i16>
    call void @llvm.masked.store.v8i16.p0v8i16(<8 x i16> %bitcast, <8 x i16>* %addr.c, i32 2, <8 x i1> %pred)
