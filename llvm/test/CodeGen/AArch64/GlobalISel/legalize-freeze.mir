# RUN: llc -march=aarch64 -run-pass=legalizer -O0 %s -o - | FileCheck %s
---
name:            test_freeze_s64
body: |
  bb.0.entry:
    liveins: $x0

    ; CHECK-LABEL: name: test_freeze_s64
    ; CHECK: %x0:_(s64) = COPY $x0
    ; CHECK: [[FREEZE:%[0-9]+]]:_(s64) = G_FREEZE %x0
    ; CHECK: $x0 = COPY [[FREEZE]](s64)
    %x0:_(s64) = COPY $x0
    %1:_(s64) = G_FREEZE %x0
    $x0 = COPY %1(s64)
...
---
