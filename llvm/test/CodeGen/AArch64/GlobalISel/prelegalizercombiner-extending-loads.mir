...

---
name:            test_anyext
body: |
  bb.0.entry:
    liveins: $x0
    ; CHECK-LABEL: name: test_anyext
    ; CHECK: [[T0:%[0-9]+]]:_(p0) = COPY $x0
    ; CHECK: [[T1:%[0-9]+]]:_(s32) = G_LOAD [[T0]](p0) :: (load 1 from %ir.addr)
    ; CHECK: $w0 = COPY [[T1]](s32)
    %0:_(p0) = COPY $x0
    %1:_(s8) = G_LOAD %0 :: (load 1 from %ir.addr)
    %2:_(s32) = G_ANYEXT %1
    $w0 = COPY %2
...

