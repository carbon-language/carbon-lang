  - { id: 0, class: gpr }
  - { id: 1, class: gpr }
  - { id: 2, class: gpr }

# CHECK:  body:
# CHECK:    %0 = COPY %w0
# CHECK:  bb.1:
# CHECK:    %2 = ADDWri %0, 1, 0
body:             |
  bb.0:
    liveins: %w0, %w1
    successors: %bb.1

    %0(s32) = COPY %w0
    %1(s32) = G_CONSTANT i32 1
    G_BR %bb.1

  bb.1:
    %2(s32) = G_ADD %0, %1
    %w0 = COPY %2(s32)
...

