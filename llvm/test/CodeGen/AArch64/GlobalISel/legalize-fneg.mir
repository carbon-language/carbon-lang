name:            test_fneg_f32
registers:
  - { id: 0, class: _ }
  - { id: 1, class: _ }
body:             |
  bb.1:
    liveins: %s0
    ; CHECK-LABEL: name: test_fneg_f32
    ; CHECK: [[COPY:%[0-9]+]]:_(s32) = COPY %s0
