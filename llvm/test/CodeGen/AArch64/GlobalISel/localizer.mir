    G_STORE %6(s32), %1(p0) :: (store 4 into @var1)
    G_STORE %4(s32), %7(p0) :: (store 4 into @var3)
    G_STORE %6(s32), %1(p0) :: (store 4 into @var1)

  bb.3.if.end:
