    $x1 = LDURXi $x0, 0
    $x0 = SUBXri $x0, 4, 0
    RET_ReallyLR implicit $x0
...
# CHECK-LABEL: name: test_STURSi_post
# CHECK: STRSpost $s0, $x0, -4
name: test_STURSi_post
body: |
  bb.0.entry:
    liveins: $x0

    $s0 = FMOVS0
    STURSi $s0, $x0, 0
    $x0 = SUBXri $x0, 4, 0
    RET_ReallyLR implicit $x0
...
