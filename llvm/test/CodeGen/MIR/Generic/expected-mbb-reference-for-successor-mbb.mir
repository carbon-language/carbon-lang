# RUN: not llc -run-pass none -o /dev/null %s 2>&1 | FileCheck %s

--- |

  define i32 @foo(i32 %a) {
  entry:
    %0 = icmp sle i32 %a, 10
    br i1 %0, label %less, label %exit

  less:
    ret i32 0

  exit:
    ret i32 %a
  }

...
---
name:            foo
body: |
  bb.0.entry:
    ; CHECK: [[@LINE+1]]:29: expected a machine basic block reference
    successors: %bb.1.less, 2

  bb.1.less:

  bb.2.exit:
...
