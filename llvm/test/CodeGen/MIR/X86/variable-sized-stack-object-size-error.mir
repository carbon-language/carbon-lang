# RUN: not llc -march=x86-64 -start-after branch-folder -stop-after branch-folder -o /dev/null %s 2>&1 | FileCheck %s
--- |

  define i32 @test(i32 %a) {
  entry:
    %b = alloca i32
    %x = alloca i64
    %y = alloca i32, i32 %a
    store i32 %a, i32* %b
    store i64 2, i64* %x
    %c = load i32, i32* %b
    ret i32 %c
  }

...
---
name:            test
frameInfo:
  stackSize:       24
  offsetAdjustment: -16
  maxAlignment:    8
  adjustsStack:    true
stack:
  - { id: 0, offset: -20, size: 4, alignment: 4 }
  - { id: 1, offset: -32, size: 8, alignment: 8 }
  # CHECK: [[@LINE+1]]:55: unknown key 'size'
  - { id: 2, type: variable-sized, offset: -32, size: 42, alignment: 1 }
body:
  - id:          0
    name:        entry
    instructions:
      - 'MOV32mr %rsp, 1, _, -4, _, %edi'
      - 'MOV64mi32 %rsp, 1, _, -16, _, 2'
      - '%eax = MOV32rm %rsp, 1, _, -4, _'
      - 'RETQ %eax'
...
