# RUN: not llc -march=x86-64 -run-pass none -o /dev/null %s 2>&1 | FileCheck %s

--- |

  define i32 @test(i32 %a) {
  body:
    ret i32 %a
  }

...
---
name:            test
tracksRegLiveness: true
registers:
  - { id: 0, class: gr32 }
liveins:
  # CHECK: [[@LINE+1]]:34: expected a virtual register
  - { reg: '$edi', virtual-reg: '$edi' }
body: |
  bb.0.body:
    liveins: $edi

    %0 = COPY $edi
    $eax = COPY %0
    RETQ $eax
...
