# RUN: llc -march=x86-64 -start-after branch-folder -stop-after branch-folder -o /dev/null %s 2>&1 | FileCheck %s
# This test ensures that the MIR parser parses X86 machine instructions
# correctly.

--- |

  define i32 @inc(i32 %a) {
  entry:
    %b = mul i32 %a, 11
    ret i32 %b
  }

...
---
# CHECK: name: inc
name:            inc
body: |
  bb.0.entry:
    ; CHECK:      MOV32rr
    ; CHECK-NEXT: RETQ
    %eax = MOV32rr %eax
     RETQ %eax
...
