# RUN: llc -march=x86-64 -start-after branch-folder -stop-after branch-folder -o /dev/null %s 2>&1 | FileCheck %s
# This test ensures that the MIR parser parses the 'killed' register flags
# correctly.

--- |

  define i32 @foo(i32 %a) {
  entry:
    %0 = icmp sle i32 %a, 10
    br i1 %0, label %less, label %exit

  less:
    ret i32 0

  exit:
    ret i32 %a
  }

...
---
name:            foo
body: |
  bb.0.entry:
    successors: %bb.1.less, %bb.2.exit

    CMP32ri8 %edi, 10, implicit-def %eflags
    JG_1 %bb.2.exit, implicit %eflags

  bb.1.less:
    ; CHECK:      %eax = MOV32r0
    ; CHECK-NEXT: RETQ killed %eax
    %eax = MOV32r0 implicit-def %eflags
    RETQ killed %eax

  bb.2.exit:
    ; CHECK:      %eax = COPY killed %edi
    ; CHECK-NEXT: RETQ killed %eax
    %eax = COPY killed %edi
    RETQ killed %eax
...
