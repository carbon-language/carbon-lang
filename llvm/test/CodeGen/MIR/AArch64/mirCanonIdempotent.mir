    %25:gpr64common = SMADDLrrr %23, %24, killed %21
    %26:gpr64sp = ADDXrx killed %25, %23, 51
    %27:fpr64 = LDURDi %26, -8
    %29:fpr64 = FADDDrr killed %27, %19
    STURDi killed %29, %26, -8

