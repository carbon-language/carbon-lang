# CHECK-NEXT:  $r5 = tMOVr $r0, 14 /* CC::al */, $noreg
# CHECK-NEXT:  $r6 = tMOVr $r0, 14 /* CC::al */, $noreg
# CHECK-NEXT:  $r7 = tMOVr $r0, 14 /* CC::al */, $noreg
