  - { id: 4, class: _ }
  - { id: 5, class: _ }
body:             |
  bb.0:
    liveins: %r0

    %4(p0) = COPY %r0

    %0(s32) = G_CONSTANT 42
    ; CHECK: {{%[0-9]+}}:_(s32) = G_CONSTANT 42

    %1(s16) = G_CONSTANT i16 21
    G_STORE %1(s16), %4(p0) :: (store 2)
    ; CHECK-NOT: G_CONSTANT i16
    ; CHECK: [[EXT:%[0-9]+]]:_(s32) = G_CONSTANT i32 21
    ; CHECK: {{%[0-9]+}}:_(s16) = G_TRUNC [[EXT]](s32)
