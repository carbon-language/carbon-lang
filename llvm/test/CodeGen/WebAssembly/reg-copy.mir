# RUN: llc -mtriple=wasm32-unknown-unknown %s -o - -run-pass=postrapseudos | FileCheck %s

---
name: copy_i32
# CHECK-LABEL: copy_i32
body: |
  ; CHECK-LABEL: bb.0:
  ; CHECK-NEXT: %0:i32 = COPY_I32 %1:i32
  ; CHECK-NEXT: RETURN
  bb.0:
    %0:i32 = COPY %1:i32
    RETURN implicit-def $arguments
...
