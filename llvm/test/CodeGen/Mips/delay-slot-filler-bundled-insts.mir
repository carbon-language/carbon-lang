    JAL @func_b, csr_n64, implicit-def dead $ra, implicit $a0_64, implicit-def $sp, implicit-def $v0_64

  bb.3.return:
    liveins: $v0_64

    $ra_64 = LD $sp_64, 8 :: (load 8 from %stack.0)
    $sp_64 = DADDiu $sp_64, 16
    PseudoReturn64 undef $ra_64, implicit $v0_64

