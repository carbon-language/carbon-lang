# RUN: llc -mtriple=amdgcn-mesa-mesa3d -run-pass=legalizer -global-isel %s -o - | FileCheck %s

--- |
  define void @test_fadd() {
  entry:
    ret void
  }

...

---
name:            test_fadd
registers:
  - { id: 0, class: _ }
  - { id: 1, class: _ }
  - { id: 2, class: _ }
body: |
  bb.0.entry:
    liveins: %vgpr0, %vgpr1
    ; CHECK-LABEL: name: test_fadd
    ; CHECK: %2:_(s32) = G_FADD %0, %1

    %0(s32) = COPY %vgpr0
    %1(s32) = COPY %vgpr1
    %2(s32) = G_FADD %0, %1
...
