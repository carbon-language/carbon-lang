# GCN-NEXT:   S_BRANCH %bb.2
# GCN:      bb.1:
# GCN-NEXT:   successors: %bb.2
# GCN-NOT:    S_AND
# GCN:        S_BRANCH %bb.2
# GCN:      bb.2:
# GCN-NEXT:   S_ENDPGM
name: two_preds_one_dead
tracksRegLiveness: true
body:             |
  bb.0:
    $vcc = IMPLICIT_DEF
    $sgpr0_sgpr1 = S_OR_B64 $exec, killed $vcc, implicit-def $scc
    S_BARRIER
    S_BRANCH %bb.2

  bb.1:
    $vcc = IMPLICIT_DEF
    $sgpr0_sgpr1 = S_AND_B64 $exec, killed $vcc, implicit-def $scc
    S_BRANCH %bb.2

  bb.2:
    S_ENDPGM
...
