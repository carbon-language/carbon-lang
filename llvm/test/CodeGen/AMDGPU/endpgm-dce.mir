    $vcc = IMPLICIT_DEF
    %0 = IMPLICIT_DEF
    %3 = IMPLICIT_DEF
    $sgpr0_sgpr1 = S_OR_B64 $exec, killed $vcc, implicit-def $scc
    %1 = FLAT_LOAD_DWORD %0, 0, 0, 0, implicit $exec, implicit $flat_scr
    %2 = V_ADD_F32_e64 0, killed %1, 0, 1, 0, 0, implicit $exec
    %4 = S_ADD_U32 %3, 1, implicit-def $scc
    S_ENDPGM
...
