    ; GFX9: %smask:sreg_32 = S_MOV_B32 65535
    ; GFX9: %vmask:vgpr_32 = V_MOV_B32_e32 65535, implicit $exec
    ; GFX9: $vgpr0 = COPY %op
    ; GFX9: $vgpr1 = COPY %op
    ; GFX9: $vgpr2 = COPY %op
    ; GFX10-LABEL: name: v_cvt_f16_f32_altmask
