    ; GFX8: %smask:sreg_32 = S_MOV_B32 65535
    ; GFX8: %vmask:vgpr_32 = V_MOV_B32_e32 65535, implicit $exec
    ; GFX8: $vgpr0 = COPY %op
    ; GFX8: $vgpr1 = COPY %op
