    $sgpr4 = IMPLICIT_DEF
    $vgpr0 = IMPLICIT_DEF
    $vgpr1 = BUFFER_LOAD_DWORD_OFFEN $vgpr0, $sgpr0_sgpr1_sgpr2_sgpr3, $sgpr4, 0, 0, 0, 0, 0, 0, implicit $exec

  bb.1:
    $sgpr0 = S_MOV_B32 0
...
