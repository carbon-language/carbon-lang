...
# GCN-LABEL: name: vmem_write_sgpr_branch_around
# GCN:      BUFFER_LOAD_DWORD_OFFEN
# GCN-NEXT: S_BRANCH
# GCN:      bb.2:
# GCN-NEXT: V_NOP
# GCN-NEXT: S_MOV_B32
---
name:            vmem_write_sgpr_branch_around
body:             |
  bb.0:
    successors: %bb.2
    $sgpr0_sgpr1_sgpr2_sgpr3 = IMPLICIT_DEF
    $sgpr4 = IMPLICIT_DEF
    $vgpr0 = IMPLICIT_DEF
    $vgpr1 = BUFFER_LOAD_DWORD_OFFEN $vgpr0, $sgpr0_sgpr1_sgpr2_sgpr3, $sgpr4, 0, 0, 0, 0, 0, 0, implicit $exec
    S_BRANCH %bb.2

  bb.1:
    successors: %bb.2
    S_WAITCNT 0

  bb.2:
    $sgpr0 = S_MOV_B32 0
...
# GCN-LABEL: name: vmem_write_sgpr_cbranch_around
# GCN:      BUFFER_LOAD_DWORD_OFFEN
# GCN-NEXT: S_CBRANCH
# GCN-NEXT: S_BRANCH
# GCN:      bb.1:
# GCN:      S_WAITCNT
# GCN:      V_ADD_I32
# GCN:      bb.2:
# GCN-NEXT: V_NOP
# GCN-NEXT: S_MOV_B32
---
name:            vmem_write_sgpr_cbranch_around
body:             |
  bb.0:
    successors: %bb.1, %bb.2
    $sgpr0_sgpr1_sgpr2_sgpr3 = IMPLICIT_DEF
    $sgpr4 = IMPLICIT_DEF
    $vgpr0 = IMPLICIT_DEF
    $vgpr1 = BUFFER_LOAD_DWORD_OFFEN $vgpr0, $sgpr0_sgpr1_sgpr2_sgpr3, $sgpr4, 0, 0, 0, 0, 0, 0, implicit $exec
    S_CBRANCH_SCC0 %bb.2, implicit $scc
    S_BRANCH %bb.1

  bb.1:
    successors: %bb.2
    S_WAITCNT 0
    $vgpr2 = V_ADD_I32_e32 $vgpr1, $vgpr1, implicit-def $vcc, implicit $exec
    S_BRANCH %bb.2

  bb.2:
    $sgpr0 = S_MOV_B32 0
...
