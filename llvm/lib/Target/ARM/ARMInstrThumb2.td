defm t2LSL  : T2I_sh_ir<0b00, "lsl", imm0_31,
                        BinOpFrag<(shl  node:$LHS, node:$RHS)>, "t2LSL">;
defm t2LSR  : T2I_sh_ir<0b01, "lsr", imm_sr,
                        BinOpFrag<(srl  node:$LHS, node:$RHS)>, "t2LSR">;
