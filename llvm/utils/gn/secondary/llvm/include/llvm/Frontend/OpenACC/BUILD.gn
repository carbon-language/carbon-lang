import("//llvm/utils/TableGen/tablegen.gni")

tablegen("ACC") {
  visibility = [ ":acc_gen" ]
  args = [ "-gen-directive-decl" ]
  output_name = "ACC.h.inc"
}

tablegen("ACCcpp") {
  visibility = [ ":acc_gen" ]
  args = [ "-gen-directive-gen" ]
  output_name = "ACC.cpp.inc"
  td_file = "ACC.td"
}

group("acc_gen") {
  deps = [
    ":ACC",
    ":ACCcpp",
  ]
}
