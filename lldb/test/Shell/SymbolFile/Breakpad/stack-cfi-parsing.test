# RUN: yaml2obj %S/Inputs/stack-cfi-parsing.yaml > %t
# RUN: %lldb -c %t -o "target symbols add %S/Inputs/stack-cfi-parsing.syms" \
# RUN:   -s %s -b | FileCheck %s

image show-unwind -n func0
# CHECK-LABEL: image show-unwind -n func0
