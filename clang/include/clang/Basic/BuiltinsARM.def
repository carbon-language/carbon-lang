// The format of this database matches clang/Basic/Builtins.def.

// In libgcc
BUILTIN(__clear_cache, "vc*c*", "")
BUILTIN(__builtin_thread_pointer, "v*", "")

// NEON
#define GET_NEON_BUILTINS
#include "clang/Basic/arm_neon.inc"
#undef GET_NEON_BUILTINS

#undef BUILTIN
