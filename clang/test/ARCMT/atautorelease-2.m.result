@interface NSAutoreleasePool
- drain;
+new;
+alloc;
-init;
-autorelease;
-release;
@end

void NSLog(id, ...);

int main (int argc, const char * argv[]) {
    @autoreleasepool {
        @autoreleasepool {

            while (argc) {
              return 0;
            }

        }
    }

    return 0;
}
