# RUN: lld -core %s | FileCheck %s

#
# Test that weak definitions are coalesced away in favor of a regular definition
#

---
defined-atoms:
    - name:         _foo
      merge:        as-weak
      scope:        global
      type:         data
---
defined-atoms:
    - name:         _foo
      scope:        global
      type:         data
---
defined-atoms:
    - name:         _foo
      merge:        as-weak
      scope:        global
      type:         data
...


# CHECK:       name: _foo
# CHECK-NOT:   merge: as-weak
# CHECK-NOT:   name: _foo
# CHECK:       ...
