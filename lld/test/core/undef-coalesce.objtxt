# RUN: lld -core %s | FileCheck %s

#
# Test that undefined symbols are coalesced with other undefined symbols
# and definitions override them.
#

---
defined-atoms:
    - name:              foo
      type:              code
      
undefined-atoms:
    - name:              malloc
    - name:              free
---
defined-atoms:
    - name:              bar
      type:              code
      
undefined-atoms:
    - name:              malloc
    - name:              myfunc
---
defined-atoms:
    - name:              myfunc
      scope:             global
      type:              code
      
undefined-atoms:
    - name:              free
...

# CHECK:  defined-atoms:
# CHECK:       name:       foo
# CHECK:       name:       bar
# CHECK:       name:       myfunc
# CHECK:       scope:      global
# CHECK:  undefined-atoms:
# CHECK:       name:       malloc
# CHECK:       name:       free
# CHECK:       ...
