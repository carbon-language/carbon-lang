# RUN: lld-core %s | FileCheck %s

#
# Test that a tentative definition and a regular global are merged into
# one regular global
#

---
atoms:
    - name:         _foo
      definition:   tentative
      scope:        global
      type:         zero-fill
      size:         4
---
atoms:
    - name:         _foo
      definition:   regular
      scope:        global
      type:         data
      content:      [ 00, 00, 00, 00 ]
...


# CHECK:        name: _foo
# CHECK-NOT:   definition: tentative
