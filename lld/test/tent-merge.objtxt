# RUN: lld -core %s | FileCheck %s

#
# Test that a tentative definition and a regular global are merged into
# one regular global
#

---
defined-atoms:
    - name:         _foo
      merge:        as-tentative
      scope:        global
      type:         zero-fill
      size:         4
---
defined-atoms:
    - name:         _foo
      scope:        global
      type:         data
      content:      [ 00, 00, 00, 00 ]
...


# CHECK:        name: _foo
# CHECK-NOT:   merge: as-tentative
