# RUN: yaml2obj -format elf %s -o %t.o
# RUN: ld.lld2 %t.o -o %t -shared
# RUN: llvm-readobj -s %t | FileCheck %s

# CHECK:      Name: .eh_frame
# CHECK-NEXT: Type: SHT_X86_64_UNWIND

FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .eh_frame
    Type:            SHT_PROGBITS
    Flags:           [ SHF_ALLOC ]
