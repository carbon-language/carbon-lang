# RUN: lld -core %s | FileCheck %s

#
# Test that dead strip attributes are preserved
#

---
defined-atoms:
    - name:         _foo1
      dead-strip:   normal
---
defined-atoms:
    - name:         _foo2
      dead-strip:   never
---
defined-atoms:
    - name:         _foo3
      dead-strip:   always
...


# CHECK:       name: _foo1
# CHECK-NOT:   dead-strip: never
# CHECK-NOT:   dead-strip: always
# CHECK:       name: _foo2
# CHECK:       dead-strip: never
# CHECK:       name: _foo3
# CHECK:       dead-strip: always
# CHECK:       ...
