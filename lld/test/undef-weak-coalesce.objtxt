# CHECK:       name:       weak_func
# CHECK:     can-be-null:  at-buildtime
# CHECK:       name:       bar1
# CHECK:       name:       bar2
# CHECK:     can-be-null:  at-runtime
# CHECK:       name:       bar3
# CHECK:     can-be-null:  at-buildtime
# CHECK:       name:       bar4
# CHECK:       name:       bar5
# CHECK:     can-be-null:  at-runtime
# CHECK:       name:       bar7
# CHECK:       name:       bar6
# CHECK:       name:       bar8
