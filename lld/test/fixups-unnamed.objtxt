# RUN: lld-core %s | FileCheck %s

#
# Test fixups to unnamed atoms
#

---
defined-atoms:
    - name:              foo
      type:              code
      content:           [ 48, 8D, 3D, 00, 00, 00, 00, 
                           48, 8D, 3D, 00, 00, 00, 00 ]
      fixups:
      - offset:          3
        kind:            3
        target:          LC1
      - offset:          10
        kind:            3
        target:          LC2
  

    - ref-name:          LC1
      scope:             hidden
      type:              c-string
      content:           [ 68, 65, 6c, 6c, 6f, 00 ]

    - ref-name:          LC2
      scope:             hidden
      type:              c-string
      content:           [ 74, 68, 65, 72, 65, 00 ]
  

...

