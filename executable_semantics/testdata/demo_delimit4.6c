fn naturals() -> () {
  var Int: n = 0;
  suspend n;
  while (true) {
    n = n + 1;
    suspend n;
  }
}

fn main() -> Int {
  var Int: sum = 0;
  var Int: count = 0;
  delimit {
    naturals();
    return 1;
  } with (v, k) {
    sum = sum + v;
    count = count + 1;
    if (count == 5) {
      return sum - 10;
    } else {
      resume k;
    }
  }
}
