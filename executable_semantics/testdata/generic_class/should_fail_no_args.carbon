// CHECK: result: 0

package ExecutableSemanticsTest api;

class Point(T:! Type) {

  // Should error, wrote `Point` instead of `Point(T)`.
  fn Origin(zero: T) -> Point {
    return {.x = zero, .y = zero};
  }

  // Should error, wrote `Point` instead of `Point(T)`.
  fn GetX[me: Point]() -> T {
    return me.x;
  }

  var x: T;
  var y: T;
}

fn Main() -> i32 {
  var p: Point(i32) = Point(i32).Origin(0);
  return p.GetX();
}
