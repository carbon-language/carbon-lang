// The behavior of this program is undefined because it tries to
// access a local variable after its lifetime is over.

fn f() -> Int* {
  var int: x;
  x = 0;
  return &x;
}

fn main() -> int {
  var Ptr(int): p;
  p = f();
  return *p;
}
