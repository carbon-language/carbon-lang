fn main() -> Int {
  var Int: x = 0;
  __delimit {
    x = x + 1;
    __yield 3;
    x = x + 2;
    return x - 6;
  } __catch (v, k) {
    x = x + v;
    __resume k;
    return 1;
  }
}
