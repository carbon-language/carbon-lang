// second bullet

template <class _R, class _T, class _T1>
inline _LIBCPP_INLINE_VISIBILITY
typename enable_if
<
    !is_base_of<_T, typename remove_reference<_T1>::type>::value,
    _R
>::type
__invoke(_R (_T::*__f)(), _T1 __t1)
{
    return ((*__t1).*__f)();
}

