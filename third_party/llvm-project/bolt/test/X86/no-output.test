  .text
  .globl  main
  .type main, %function
  .p2align  4
main:
# FDATA: 0 [unknown] 0 1 main 0 0 510
  pushq %rbp
  movq  %rsp, %rbp
  subq  $0x18, %rsp
  addq  $0x18, %rsp
  xorq  %rax, %rax
  leaveq
  retq
.size main, .-main


