#define Something_Macro(key, comment) \
 [[Foo new] something]

@implementation Foo

@synthesize bar;

-(id)something {}

-(id)test:(id)obj {
  id x = self.bar;
  self.bar = obj;

  Something_Macro(@"foo", "@bar");

  IhaveSideEffect();

  [self something];

  [self something];

  IhaveSideEffect();
  // do stuff with x;
  return self;
}
  
- (id)test1 {
  id x=0;
  return (((x)));
}
