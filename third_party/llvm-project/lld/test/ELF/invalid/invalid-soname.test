# REQUIRES: x86
# RUN: yaml2obj %s -o %t.so
# RUN: not ld.lld %t.so -o /dev/null 2>&1 | FileCheck %s

# CHECK: error: {{.*}}.so: invalid DT_SONAME entry

--- !ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_DYN
  Machine:         EM_X86_64
Sections:
  - Name:          .test
    Type:          SHT_DYNAMIC
    Flags:         [ SHF_ALLOC ]
    Link:          .strtab
    EntSize:       0x0000000000000010
    Entries:       
      - Tag:         DT_SONAME
        Value:       0x0100000000000000
