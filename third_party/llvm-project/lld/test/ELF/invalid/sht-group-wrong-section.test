# REQUIRES: x86
# RUN: yaml2obj %s -o %t.o
# RUN: not ld.lld %t.o %t.o -o /dev/null 2>&1 | FileCheck %s
# CHECK: error: {{.*}}.o: invalid section index in group: 12345

--- !ELF
FileHeader:
  Class:               ELFCLASS64
  Data:                ELFDATA2LSB
  Type:                ET_REL
  Machine:             EM_X86_64
Sections:
  - Name:              .group
    Type:              SHT_GROUP
    Link:              .symtab
    Info:              foo
    Members:
      - SectionOrType: GRP_COMDAT
      - SectionOrType: 12345
Symbols:
  - Name:            foo
    Binding:         STB_GLOBAL
