static_library("JIT") {
  output_name = "lldbPluginObjectFileJIT"
  configs += [ "//llvm/utils/gn/build:lldb_code" ]
  deps = [
    "//lldb/source/Core",
    "//lldb/source/Host",

    #"//lldb/source/Symbol", # 2-hop dependency cycle.
