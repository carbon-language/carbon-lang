# RUN: yaml2obj %p/Inputs/i386-slice.yaml -o %t-i386.o
# RUN: yaml2obj %p/Inputs/CPU14-slice.yaml -o %t-CPU14.o
# RUN: yaml2obj %p/Inputs/CPU10-slice.yaml -o %t-CPU10.o

# RUN: llvm-lipo %t-i386.o %t-CPU14.o %t-CPU10.o -create -output %t-universal.o

# RUN: llvm-objdump %t-universal.o -m --universal-headers | FileCheck %s
# CHECK: fat_magic FAT_MAGIC
# CHECK: nfat_arch 3
# CHECK: architecture
# CHECK: cputype (10)
# CHECK: offset 72
# CHECK: align 2^3 (8)
# CHECK: architecture
# CHECK: cputype (14)
# CHECK: offset 8544
# CHECK: align 2^4 (16)
# CHECK: architecture i386
# CHECK: offset 12288
# CHECK: align 2^12 (4096)
