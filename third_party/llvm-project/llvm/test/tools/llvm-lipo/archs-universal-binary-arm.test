# RUN: yaml2obj %s -o %t
# RUN: llvm-lipo %t -archs | FileCheck %s
# CHECK: armv7k arm64

--- !fat-mach-o
FatHeader:
  magic:           0xCAFEBABE
  nfat_arch:       2
FatArchs:
  - cputype:         0x0000000C
    cpusubtype:      0x0000000C
    offset:          0x0000000000004000
    size:            28
    align:           14
  - cputype:         0x0100000C
    cpusubtype:      0x00000000
    offset:          0x000000000001C000
    size:            32
    align:           14
Slices:
  - !mach-o
    FileHeader:
      magic:           0xFEEDFACE
      cputype:         0x0000000C
      cpusubtype:      0x0000000C
      filetype:        0x00000002
      ncmds:           0
      sizeofcmds:      0
      flags:           0x00200085
  - !mach-o
    FileHeader:
      magic:           0xFEEDFACF
      cputype:         0x0100000C
      cpusubtype:      0x00000000
      filetype:        0x00000002
      ncmds:           0
      sizeofcmds:      0
      flags:           0x00210085
      reserved:        0x00000000
...
