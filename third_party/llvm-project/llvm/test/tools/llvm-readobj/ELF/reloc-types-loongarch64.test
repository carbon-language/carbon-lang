      - Type: R_LARCH_TLS_DTPREL64
      - Type: R_LARCH_TLS_TPREL32
      - Type: R_LARCH_TLS_TPREL64
      - Type: R_LARCH_IRELATIVE
      - Type: R_LARCH_MARK_LA
      - Type: R_LARCH_MARK_PCREL
      - Type: R_LARCH_SOP_PUSH_PCREL
      - Type: R_LARCH_SOP_PUSH_ABSOLUTE
