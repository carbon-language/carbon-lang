      - Type: R_LARCH_SOP_POP_32_S_10_12
      - Type: R_LARCH_SOP_POP_32_S_10_16
      - Type: R_LARCH_SOP_POP_32_S_10_16_S2
      - Type: R_LARCH_SOP_POP_32_S_5_20
      - Type: R_LARCH_SOP_POP_32_S_0_5_10_16_S2
