#CHECK-NEXT:  }
#CHECK-NEXT:  Symbol {
#CHECK-NEXT:    Name: _start
#CHECK-NEXT:    Value: 0x1000
#CHECK-NEXT:    Size: 4
#CHECK-NEXT:    Binding: Global
#CHECK-NEXT:    Type: Function
#CHECK-NEXT:    Other: 0
#CHECK-NEXT:    Section: .text
#CHECK-NEXT:  }
#CHECK-NEXT:  Symbol {
