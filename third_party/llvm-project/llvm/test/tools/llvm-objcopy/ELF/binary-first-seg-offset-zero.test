# RUN: yaml2obj %s -o %t
# RUN: llvm-objcopy -R .note -O binary %t %t2
# RUN: od -Ax -t x1 %t2 | FileCheck %s

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_EXEC
  Machine:         EM_X86_64
Sections:
  - Name:            .note
    Type:            SHT_PROGBITS
    Flags:           [ SHF_ALLOC ]
    Address:         0x1000
    AddressAlign:    0x1000
    Content:         "32323232"
    Size:            32
  - Name:            .rodata
    Flags:           [ SHF_ALLOC ]
    Type:            SHT_PROGBITS
    Address:         0x1020
    Size:            4064
    Content:         "DEADBEEF"
ProgramHeaders:
  - Type:     PT_LOAD
    Flags:    [ PF_R ]
    VAddr:    0x1000
    FirstSec: .note
    LastSec:  .rodata

# CHECK: 000000 de ad be ef
