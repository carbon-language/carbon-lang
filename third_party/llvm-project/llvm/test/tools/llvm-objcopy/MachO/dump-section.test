    vmaddr:          0
    vmsize:          12
    fileoff:         344
    filesize:        12
    maxprot:         7
    initprot:        7
    nsects:          3
    flags:           0
    Sections:
      - sectname:        __text
        segname:         __TEXT
        addr:            0x0000000000000000
        content:         'AABBCCDD'
        size:            4
        offset:          344
        align:           0
        reloff:          0x00000000
        nreloc:          0
        flags:           0x80000400
        reserved1:       0x00000000
        reserved2:       0x00000000
      - sectname:        __data
        segname:         __DATA
        addr:            0x0000000000000004
        content:         'EEFFEEFF'
        size:            4
        offset:          348
        align:           0
        reloff:          0x00000000
        nreloc:          0
        flags:           0x00000000
        reserved1:       0x00000000
        reserved2:       0x00000000
      - sectname:        __const
        segname:         __TEXT
        addr:            0x0000000000000008
        content:         'EEFFEEFF'
        size:            4
        offset:          352
        align:           0
        reloff:          0x00000000
