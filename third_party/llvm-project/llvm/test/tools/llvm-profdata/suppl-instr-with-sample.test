RUN:     -supplement-instr-with-sample=%p/Inputs/mix_sample.proftext \
RUN:     -suppl-min-size-threshold=2 -zero-counter-threshold=0.7 \
RUN:     -instr-prof-cold-threshold=30 %p/Inputs/mix_instr.proftext -o %t
