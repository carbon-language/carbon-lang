# RUN: llc -verify-machineinstrs -mtriple aarch64--- \
# RUN: -run-pass=instruction-select -mattr=+fullfp16 -global-isel %s -o - \
# RUN: | FileCheck %s
...
---
name:            fabs_float
legalized:       true
regBankSelected: true
tracksRegLiveness: true
registers:
  - { id: 0, class: fpr }
  - { id: 1, class: fpr }
body:             |
  bb.0:
    ; CHECK-LABEL: name:            fabs_float
    ; CHECK: %{{[0-9]+}}:fpr32 = FABSSr %{{[0-9]+}}
    liveins: $s0
    %0:fpr(s32) = COPY $s0
    %1:fpr(s32) = G_FABS %0
    $s0 = COPY %1(s32)

...
