    ; CHECK: $w0 = COPY [[COPY1]](s32)
    ; CHECK: $w0 = COPY [[AND]](s32)
    %0:_(s64) = COPY $x0
    %1:_(s8) = G_TRUNC %0(s64)
    %19:_(s32) = G_ZEXT %1(s8)
