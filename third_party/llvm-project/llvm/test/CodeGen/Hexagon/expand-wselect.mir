# RUN: llc -march=hexagon -run-pass postrapseudos %s -o - | FileCheck %s

# Check that this doesn't crash.
# CHECK: $w2 = V6_vccombine $p0, $v1, $v0
# CHECK: $w2 = V6_vnccombine killed $p0, $v3, $v2, implicit $w2

name: fred
