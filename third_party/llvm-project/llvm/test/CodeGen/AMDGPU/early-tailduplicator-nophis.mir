name:           tail_duplicate_nophis
tracksRegLiveness: true
body:             |
  ; CHECK-LABEL: name: tail_duplicate_nophis
  ; CHECK: bb.0:
  ; CHECK:   successors: %bb.3(0x80000000)
  ; CHECK:   [[S_MOV_B32_:%[0-9]+]]:sreg_32 = S_MOV_B32 0
