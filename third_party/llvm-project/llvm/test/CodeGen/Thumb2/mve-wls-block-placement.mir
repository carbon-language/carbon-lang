    tCMPi8 killed renamable $r0, 0, 14 /* CC::al */, $noreg, implicit-def $cpsr
    t2Bcc %bb.1, 0 /* CC::eq */, killed $cpsr

  bb.2:
    successors: %bb.3(0x80000000)
