package ExplorerTest api;

interface Vector {
  fn Add[me: Self](b: Self) -> Self;
}

fn ScaleGeneric[T:! Vector](a: T, s: i32) -> T {
  // CHECK: COMPILATION ERROR: {{.*}}/explorer/testdata/interface/fail_interface_missing_member.carbon:[[@LINE+1]]: member access, Scale not in interface Vector
  return a.Scale(s);
}

class Point {
  var x: i32;
  var y: i32;
  impl Point as Vector {
    fn Add[me: Point](b: Point) -> Point {
      return {.x = me.x + b.x, .y = me.y + b.y};
    }
  }
}

fn Main() -> i32 {
  var a: Point = {.x = 3, .y = 1};
  var b: Point = ScaleGeneric(a, 2);
  return b.x - 6;
}
