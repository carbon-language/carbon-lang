package ExplorerTest api;

class Point(T:! type) {
  // Allowed: `Self` means `Point(T)` here.
  fn Origin(zero: T) -> Self {
    return {.x = zero, .y = zero};
  }

  fn GetX[self: Self]() -> T {
    return self.x;
  }

  var x: T;
  var y: T;
}

fn Main() -> i32 {
  var p: Point(i32) = Point(i32).Origin(0);
  return p.GetX();
}

