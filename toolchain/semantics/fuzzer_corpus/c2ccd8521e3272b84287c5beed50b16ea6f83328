// AUTOUPDATE
// CHECK:STDOUT: i32.F
// CHECK:STDOUT: result: 0

package ExplorerTest api;

interface Base {
  fn F();
}

interface Extension {
  extend Base;
}

fn F[T:! type where .Self impls Extension](x: T) {
  x.(Extension.F)();
}

impl i32 as Extension {
  fn F() { Print("i32.F"); }
}

fn Main() -> i32 {
  var n: i32 = 0;
  F(n);
  return 0;
}
