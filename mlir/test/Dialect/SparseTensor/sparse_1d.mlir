    ins(%arga, %argb, %argc, %argd: tensor<?xf64>, tensor<?xf64, #SV>, tensor<?xf64>, tensor<?xf64, #SV>)
    outs(%argx: tensor<?xf64>) {
      ^bb(%a: f64, %b: f64, %c: f64, %d: f64, %x: f64):
        %0 = addf %a, %b : f64
        %1 = addf %c, %d : f64
        %2 = addf %0, %1 : f64
        linalg.yield %2 : f64
